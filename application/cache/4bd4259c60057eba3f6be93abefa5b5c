    <script>

            $('.add_to_wishlist').on('click', function(e){
          e.preventDefault();
        var id  = $(this).data("id");
         $.ajax({
            type: "post",
            url:'https://correos.instaone.eu/add_to_wishlist',
            data:{id},       
            success: function(response){ 

                $.ajax({
                        type: "post",
                        url:'https://correos.instaone.eu/wishlist_count',
                        data:{id},
                        cache: false,       
                        success: function(response){ 

                            $('.wish-list').html(response); 
                        },
                
                    });       
                },
            error: function(){            
              alert('Error while request..');
            }
        });
     });

        $(function() {
    
            var availableTags = [];
        $( "#tags" ).autocomplete({


            source: function (request, response)
    {

        $.ajax(
        {
           
            type:'POST',
            url:'https://correos.instaone.eu/get_search_val',
            data:
            {
                term: request.term,
                cats : $('.cats :selected').val()
            },
            success:function(data)
            {
               data = jQuery.parseJSON(data);
                response(data);


            }
        });
    },
    minLength: 2,

     change: function( event, ui ) {},
                            autoFocus: true,
                            select: function (event, ui) {
                                value  = encodeURI(ui.item.label) ;
            window.open("https://correos.instaone.eu/products/?s="+value+""); 

                           },
        });  



$("select.cats").change(function(){
var cats = $('#tags').val('');
  

    });
           
        });
    </script>
