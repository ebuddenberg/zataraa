<script type="text/javascript">
  let firedEvents = [];

$(window).scroll(function() {
  $("div.auto-scroll").each(function() {
    if (!firedEvents.includes(this)  && $(window).scrollTop() > $(this).offset().top) {
     
      firedEvents.push(this);
      if($(this).data("id") == "section_one"){

          $.ajax({
            type: "post",
            url:'https://correos.instaone.eu/get_section_one',
            cache: false,       
            success: function(data){ 
                $( "#product-box-e-1").html(data);
            }
        });

        $.ajax({
            type: "post",
            url:'https://correos.instaone.eu/recomended_product_one',
            cache: false,       
            success: function(data){ 
                $( "#recomended_product_one").html(data);
            }
        });
      }
      if($(this).data("id") == "section_two"){

          $.ajax({
            type: "post",
            url:'https://correos.instaone.eu/get_section_two',
            cache: false,       
            success: function(data){ 
                $( "#product-box-c-1").html(data);
            }
        });

        $.ajax({
            type: "post",
            url:'https://correos.instaone.eu/recomended_product_two',
            cache: false,       
            success: function(data){ 
                $( "#recomended_product_two").html(data);
            }
        });
      }

       if($(this).data("id") == "section_three"){

          $.ajax({
            type: "post",
            url:'https://correos.instaone.eu/get_section_three',
            cache: false,       
            success: function(data){ 
                $( "#product-box-h-1").html(data);
            }
        });

        $.ajax({
            type: "post",
            url:'https://correos.instaone.eu/recomended_product_three',
            cache: false,       
            success: function(data){ 
                $( "#recomended_product_three").html(data);
            }
        });
      }
    if($(this).data("id") == "our_collection"){
      $.ajax({
            type: "post",
            url:'https://correos.instaone.eu/our_collection',
            cache: false,       
            success: function(data){ 
                $( "#our_collection").html(data);
            }
        });
    }

 if($(this).data("id") == "banner_slider"){

     $.ajax({
            type: "post",
            url:'https://correos.instaone.eu/search_trending',
            cache: false,       
            success: function(data){ 
               // $( "#search_trending").html(data);
                $( "#search_trending > .owl-stage-outer > .owl-stage" ).empty();
          $( "#search_trending > .owl-stage-outer > .owl-stage" ).append(data);
            }
        });

  $.ajax({
            type: "post",
            url:'https://correos.instaone.eu/search_trending_tab',
            cache: false,       
            success: function(data){ 
                $( "#search_trending_tab").html(data);
            }
        });
}
 if($(this).data("id") == "search_trending"){

    $.ajax({
            type: "post",
            url:'https://correos.instaone.eu/deals_of_the_weak',
            cache: false,       
            success: function(data){ 
                $( "#deal_of_the_Week").html(data);
            }
        });

  }

      

    }
  });
});
   $('search_trending').on('click', ' li > a',function(e) {
            e.preventDefault();
            var target = $(this).attr('href');
            $(this).closest('li').siblings('li').removeClass('active');
            $(this).closest('li').addClass('active');
            $(target).addClass('active');
            $(target).siblings('.ps-tab').removeClass('active');
        });
 $('#search_trending').on('click', '.owl-item a', function(e){
            e.preventDefault();
            var target = $(this).attr('href');
            $(this).closest('.owl-item').siblings('.owl-item').removeClass('active');
            $(this).closest('.owl-item').addClass('active');
            $(target).addClass('active');
            $(target).siblings('.ps-tab').removeClass('active');
        });
 $('.load-more').on('click', function(e){
            e.preventDefault();
            var id = $(this).attr('data-id');
              $.ajax({
            type: "post",
            url:'https://correos.instaone.eu/our_collection',
            cache: false,
            data : id,       
            success: function(data){ 
                $( "#our_collection").append(data);
            }
        });
        });

</script>
